{
  "name": "@actionsflow/trigger-pocket",
  "version": "1.0.0",
  "description": "Actionsflow trigger example",
  "keywords": [
    "pocket",
    "getpocket",
    "actionsflow",
    "trigger"
  ],
  "author": "Joseph Chiocchi <joe@yolk.cc>",
  "homepage": "https://github.com/yyolk/actionsflow-trigger-pocket",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyolk/actionsflow-trigger-pocket.git"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "buildProd": "tsc -p tsconfig.production.json && sed -i 's|export default class|class|' dist/src/index.js && sed -i \"$(( $(wc -l < dist/src/index.js)-1+2 )),$ d\" dist/src/index.js && echo 'exports.default = Pocket;' >> dist/src/index.js && echo '//# sourceMappingURL=index.js.map' >> dist/src/index.js",
    "clean": "rimraf ./dist",
    "lint": "prettier --check ./src",
    "format": "prettier --write ./src",
    "prepublishOnly": "npm run buildProd",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/yyolk/actionsflow-trigger-pocket/issues"
  },
  "dependencies": {
    "actionsflow-core": "^1.5.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
